#include <iostream>
using namespace std;

int main() {
    int a,b,rem;
    cout<<"Emter the two numbers to get gcd";
    cin>>a>>b;
    while(b!=0){
        rem=a%b;
        a=b;
        b=rem;

    }

    cout<<a;
    //second method
class sol{
public:
if(a==0)
return b;
if(b==0)
return a;
while(a!=0){
if(a>b)
a=a-b;
else
b=b-a;
}
return a;
}

}
}
