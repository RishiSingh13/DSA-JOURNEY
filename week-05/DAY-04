#include <iostream>
using namespace std;
class node{
    public:
    int value;
    node * next;
    node(int value){
        this->value=value;
        this->next=NULL;
    }
};
class list{
    node * head;
    node* tail;
    public:
     list(){
        head=NULL;
     }
     void push_front(int val){
        node * newnode=new node(val);
        if(head==NULL){
            head=tail=newnode;
        }
        else{
            newnode->next=head;
            head=newnode;
        }
     }
     void push_back(int val){
        node * newnode=new node(val);
        if(head==NULL){
            head=tail=newnode;
        }
        else{
            tail->next=newnode;
            tail=newnode;
        }

     }
     public:
     void print(){
        node * temp;
        temp=head;
        while(temp!=NULL){
            cout<<temp->value<<endl;
            temp=temp->next;
        }
     }
};
int main() {
    list l1;
    l1.push_front(2);
    l1.push_front(22);
    l1.push_front(32);
    l1.push_back(9);
    l1.print();
    return 0;
}
