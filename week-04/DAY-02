class Solution {
public:
    bool isAnagram(string s, string t) {
        vector<int>alpha1(26,0);
        vector<int>alpha2(26,0);
        for(int i=0;i<s.length();i++){
            int num = s[i]-'a';
            alpha1[num]++;
        }
        for(int i=0;i<t.length();i++){
            int num = t[i]-'a';
            alpha2[num]++;
        }
        if(alpha1==alpha2)
        return true;
        else
        return false;
    }
};
