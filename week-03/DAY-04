#include <iostream>
using namespace std;

int main() {
    int a, n;
    long long result = 1;

    cout << "Enter the number and power respectively: ";
    cin >> a >> n;

    while (n > 0) {
        if (n & 1) {          
            result *= a;
        }
        a *= a;               
        n = n >> 1;           
    }

    cout << result;
    return 0;
}
